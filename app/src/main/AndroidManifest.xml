<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vetdevelopers.vetnetwork">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".BrowseActivity2"></activity>
        <activity android:name=".ChangePasswordActivity" />
        <activity android:name=".BrowseActivity" />
        <activity android:name=".SearchActivity" />
        <activity
            android:name=".AdminPanelActivity"
            android:label="@string/title_activity_admin_control_panel"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AdminProfileActivity"
            android:label="@string/title_activity_admin_profile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EditProfileActivity"
            android:label="@string/title_activity_profile_setting"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ProfileActivity"
            android:label="@string/title_activity_nav_profile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StartPageActivity" />
        <activity
            android:name=".RegistrationActivity"
            android:parentActivityName=".StartPageActivity" />
        <activity android:name=".ViewProfileActivity" />
    </application>

</manifest>